
@{
    ViewBag.Title = "forumcommentShield";
}

@model Lazyfitness.Models.postReply

<h2>forumcommentShield</h2>
<h2>论坛评论屏蔽</h2>

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

<form method="post" action="~/backStage/commentManagement/forumcommentShield">
    <label>论坛帖子ID</label> @Html.TextBoxFor(u => u.postId) @Html.ValidationMessageFor(x => x.postId)<br />
    <label>评论者ID</label> @Html.TextBoxFor(u => u.userId) @Html.ValidationMessageFor(x => x.userId)<br />
    <input type="submit" id="postData" name="postData" value="屏蔽" />
</form>
<br /><a href="~/backStage/commentManagement/Index">返回</a><br />